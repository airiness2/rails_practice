<h3>以下の内容で、作成する。</h3>

<%= form_with(model: @contact, url: new_contact_path, local: true) do |form| %>
<%= form.hidden_field :name %>
<%= form.hidden_field :email %>
<%= form.hidden_field :content %>

<p>名前: <%= @contact.name %></p>
<p>メールアドレス: <%= @contact.email %></p>
<p>本文: <%= @contact.content %></p>

<%= form.submit "登録する" %>

<% end %>
